{
	"name": "New Tab Page",
	"version": "1.1.0",
	"manifest_version": 2,
	"description": "Display a blank page when opening new tabs.",
	"chrome_url_overrides": {
		"newtab": "blank.html"
	},
	"content_security_policy": "default-src 'none'",
	"incognito": "not_allowed",
	"externally_connectable": {
		"ids": []
	}
}
